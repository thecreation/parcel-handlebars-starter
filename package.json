{
  "name": "parcel-handlebars-starter",
  "version": "0.0.1",
  "description": "A starter template for Parcel, Handlebars, ECMAScript(latest), sass and postcss.",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "format": "prettier --write src/**/*.{js,json,scss,md}",
    "build": "npm run clean && parcel build src/index.html --no-source-maps --no-cache",
    "start": "parcel src/index.html --open --no-cache",
    "watch": "parcel watch src/index.html",
    "lint:es": "eslint --ignore-path .gitignore \"src/**/*.js\"",
    "lint:style": "stylelint \"src/**/*.scss\"",
    "lint": "npm run lint:es && npm run lint:style"
  },
  "author": "Creation Studio Limited",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-minify": "^0.5.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.5.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "node-sass": "^7.0.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-handlebars": "^0.4.4",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "stylelint": "^10.1.0",
    "stylelint-config-idiomatic-order": "^7.0.3",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-scss": "^3.9.2"
  },
  "browserslist": [
    "> 1%",
    "ie >= 11",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "dependencies": {
    "autoprefixer": "^9.6.1",
    "bootstrap": "^4.3.1",
    "jquery": "^3.4.1",
    "parcel-bundler": "^1.12.3",
    "popper.js": "^1.15.0",
    "rimraf": "^2.6.3"
  }
}
